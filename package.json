{
  "name": "cognicity-server",
  "version": "3.0.0",
  "description": "Data Server for CogniCity",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "mocha": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "https://github.com/urbanriskmap/cognicity-server-v3",
  "issues": "https://github.com/urbanriskmap/cognicity-server-v3/issues",
  "author": "Tomas Holderness et al., MIT Urban Risk Lab",
  "license": "GPL-3.0",
  "dependencies": {
    "api-error-handler": "^1.0.0",
    "apicache": "^0.8.3",
    "bluebird": "^3.4.7",
    "body-parser": "^1.16.0",
    "celebrate": "^4.0.1",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "dbgeo": "^1.0.1",
    "dotenv": "^4.0.0",
    "express": "^4.14.1",
    "express-jwt": "^5.1.0",
    "joi": "^10.2.1",
    "jwks-rsa": "^1.1.1",
    "moment-timezone": "^0.5.11",
    "morgan": "^1.8.0",
    "pg-promise": "^5.5.6",
    "response-time": "^2.3.2",
    "shortid": "^2.2.6",
    "winston": "^2.3.1",
    "xmlbuilder": "^8.2.2"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "chai": "^3.5.0",
    "eslint": "^3.15.0",
    "it-each": "^0.3.1",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "supertest": "^3.0.0"
  },
  "engines": {
    "node": ">=6.9.1"
  }
}
